.navbar
  .navbar-inner
    = link_to "ContactExchange", root_path, :class => "brand"
    %ul.nav.nav-pills.pull-right
      - if !current_user
        %li= link_to "Log in", user_omniauth_authorize_path(:google_oauth2)
      - else
        %li= link_to current_user.email, account_path
        %li= link_to "Log out", destroy_user_session_path, method: :delete